<!-- progress steps  -->



<link href="//netdna.bootstrapcdn.com/font-awesome/3.2.1/css/font-awesome.css" rel="stylesheet">
<div id="steps">
  <div class="step active" data-desc="Select Patient">1</div>
  <div class="step " data-desc="Select Meds">2</div>
  <div class="step" data-desc="Education">3</div>
  <div class="step" data-desc="Teach Back">4</div>
</div>

<script>
$(document).ready( function() {
  $('.step').each(function(index, element) {
    // element == this
    $(element).not('.active').addClass('done');
    $('.done').html('<i class="icon-ok"></i>');
    if($(this).is('.active')) {
      return false;
    }
  });
});
</script>
<!-- end progress steps -->

<!-- line break -->
<hr style="border:none;border-top:1px solid rgba(102, 102, 102, 0.16);border-bottom:1px solid rgba(204, 204, 204, 0);" />
